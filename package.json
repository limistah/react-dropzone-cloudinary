{
  "name": "react-dropzone-cloudinary",
  "version": "1.0.0-0",
  "description": "",
  "main": "dist/es/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "cross-env NODE_ENV=test npm run eslint:src && jest --coverage && npm run typescript",
    "build": "npm run clean && npm run build:es && npm run build:umd && cp ./dist/bundle.css ./dist/es/style.css",
    "build:umd": "cross-env NODE_ENV=es rollup -c",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./dist/es --ignore '**/*.spec.js'",
    "start": "styleguidist server",
    "styleguide": "styleguidist build",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "eslint:src": "eslint .",
    "commitmsg": "commitlint -e",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "logo": "cd logo && sketchtool export artboards logo.sketch",
    "imagemin": "imagemin --out-dir=logo --plugin=pngquant --plugin=svgo",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "typescript": "tsc --project ./typings/tests"
  },
  "size-limit": [
    {
      "path": "src/index.js",
      "limit": "117 KB"
    }
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{svg,png}": [
      "imagemin",
      "git add"
    ]
  },
  "jest": {
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/testSetup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/dist/",
      "/node_modules/",
      "<rootDir>/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "keywords": [
    "react-component",
    "react",
    "drag",
    "drop",
    "upload",
    "cloudinary"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">= 16.8"
  },
  "dependencies": {
    "attr-accept": "^1.1.3",
    "axios": "^0.19.2",
    "eslint-config-airbnb": "^17.1.1",
    "file-selector": "^0.1.11",
    "jest": "^3.1.2",
    "jest-dom": "^3.1.2",
    "prop-types": "^15.7.2",
    "react-dropzone": "^11.0.1",
    "react-sweet-progress": "^1.1.2",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-json": "^4.0.0",
    "sha1": "^1.1.1",
    "webpack-blocks": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/prompt": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "babel-eslint": "*",
    "babel-jest": "^24.1.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "eslint": "4.x",
    "eslint-config-okonet": "^7.0.2",
    "eslint-config-prettier": "2.x",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "5.x",
    "eslint-plugin-prettier": "2.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "^1.0.2",
    "husky": "^0.14.3",
    "imagemin-cli": "^3.0.0",
    "imagemin-pngquant": "^6.0.0",
    "lint-staged": "^7.2.2",
    "markdownlint-cli": "^0.13.0",
    "prettier": "*",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-hooks-testing-library": "^0.3.4",
    "react-styleguidist": "^9.0.1",
    "react-test-renderer": "^16.8.2",
    "react-testing-library": "^6.0.0",
    "rimraf": "^2.5.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sinon": "^3.2.1",
    "size-limit": "^0.19.2",
    "style-loader": "^0.18.2",
    "styled-components": "^4.1.2",
    "tslint": "^5.9.1",
    "typescript": "^3.2.4",
    "webpack": "^4.29.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/limistah/react-video-trimmer.git"
  },
  "typings": "typings/react-dropzone.d.ts",
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "engines": {
    "node": ">= 8"
  }
}
